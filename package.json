{
  "name": "fdstruct",
  "version": "0.2.1",
  "description": "A CLI for managing symlinks and folder structures using templates",
  "author": "Horatzio",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/Horatzio/fdstruct-cli.git"
  },
  "keywords": [
    "fdstruct",
    "cli",
    "symlink",
    "symlinks",
    "hardlink",
    "hardlinks",
    "folder-structure",
    "terminal",
    "yaml",
    "yml",
    "commander",
    "commanderjs",
    "command-line-tool",
    "command-line",
    "command line",
    "cl",
    "node",
    "nodejs"
  ],
  "engines": {
    "node": ">=18.16.0"
  },
  "bin": {
    "fdstruct": "./dist/fdstruct.js"
  },
  "directories": {
    "bin": "./dist"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "predev": "npm run clean",
    "dev": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript -w --configDebug",
    "prebundle": "npm run clean",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "clean": "rimraf dist",
    "prerelease": "npm run prettify",
    "release": "npm run bundle && npm version minor && git push && npm publish",
    "prettify": "npm run lint && npm run prettier",
    "lint": "eslint --fix ./src",
    "prettier": "prettier \"**/*.{ts,json,md}\" --write",
    "test": "vitest run --test-timeout=180000",
    "test:dev": "vitest --test-timeout=1200000 --silent=false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "micromatch": "^4.0.5",
    "parse-gitignore": "^2.0.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/fs-extra": "^11.0.2",
    "@types/micromatch": "^4.0.3",
    "@types/mock-fs": "^4.13.2",
    "@types/node": "^18.16.0",
    "eslint": "^4.19.1",
    "husky": "^1.0.0-rc.7",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-add-shebang": "^0.3.1",
    "ts-loader": "^9.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^1.3.1"
  }
}